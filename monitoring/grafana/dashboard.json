    {
      "id": 23,
      "title": "Channel Join Success Rate",
      "type": "graph",
      "targets": [
        {
          "expr": "sum(rate(twitch_scraper_channel_join_attempts_total{status=\"success\"}[5m])) / sum(rate(twitch_scraper_channel_join_attempts_total[5m])) * 100",
          "legendFormat": "Success Rate %",
          "refId": "A"
        }
      ],
      "yaxes": [
        {
          "format": "percent",
          "label": "Success Rate",
          "min": 0,
          "max": 100
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 60
      }
    },
    {
      "id": 24,
      "title": "Message Buffer Size vs Capacity",
      "type": "graph",
      "targets": [
        {
          "expr": "twitch_scraper_message_buffer_size",
          "legendFormat": "Current Size",
          "refId": "A"
        },
        {
          "expr": "twitch_scraper_message_buffer_capacity",
          "legendFormat": "Capacity",
          "refId": "B"
        }
      ],
      "yaxes": [
        {
          "format": "short",
          "label": "Messages"
