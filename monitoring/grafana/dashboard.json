{
  "id": null,
  "title": "Twitch Chat Scraper Dashboard",
  "tags": ["twitch", "scraper", "prometheus"],
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "title": "Total Messages Received",
      "type": "stat",
      "targets": [
        {
          "expr": "twitch_scraper_messages_total",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          },
          "unit": "short"
        }
      },
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "title": "Messages Per Second (Rate)",
      "type": "graph",
      "targets": [
        {
          "expr": "rate(twitch_scraper_messages_total[1m])",
          "legendFormat": "Messages/sec",
          "refId": "A"
        }
      ],
      "yaxes": [
        {
          "format": "short",
          "label": "Messages/sec"
        }
      ],
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 6,
        "y": 0
      }
    },
    {
      "id": 3,
      "title": "Active Connections",
      "type": "stat",
      "targets": [
        {
          "expr": "twitch_scraper_active_connections",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "yellow",
                "value": 1
              },
              {
                "color": "green",
                "value": 3
              }
            ]
          },
          "unit": "short"
        }
      },
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 12,
        "y": 0
      }
    },
    {
      "id": 4,
      "title": "Joined Channels Total",
      "type": "stat",
      "targets": [
        {
          "expr": "twitch_scraper_joined_channels_total",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          },
          "unit": "short"
        }
      },
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 18,
        "y": 0
      }
    },
    {
      "id": 5,
      "title": "Messages by Type (Rate)",
      "type": "graph",
      "targets": [
        {
          "expr": "sum by (message_type) (rate(twitch_scraper_parsed_messages_by_type_total[5m]))",
          "legendFormat": "{{message_type}}",
          "refId": "A"
        }
      ],
      "yaxes": [
        {
          "format": "short",
          "label": "Messages/sec"
        }
      ],
      "legend": {
        "show": true,
        "values": true,
        "current": true
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 6
      }
    },
    {
      "id": 6,
      "title": "Messages by Type (Total Count)",
      "type": "piechart",
      "targets": [
        {
          "expr": "sum by (message_type) (twitch_scraper_parsed_messages_by_type_total)",
          "legendFormat": "{{message_type}}",
          "refId": "A"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "values": ["value", "percent"]
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 6
      }
    },
    {
      "id": 7,
      "title": "Messages Per Connection",
      "type": "graph",
      "targets": [
        {
          "expr": "rate(twitch_scraper_messages_per_connection_total[5m])",
          "legendFormat": "{{connection_id}}",
          "refId": "A"
        }
      ],
      "yaxes": [
        {
          "format": "short",
          "label": "Messages/sec"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 14
      }
    },
    {
      "id": 8,
      "title": "Connection Status",
      "type": "table",
      "targets": [
        {
          "expr": "twitch_scraper_connection_status",
          "format": "table",
          "instant": true,
          "refId": "A"
        }
      ],
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "__name__": true,
              "instance": true,
              "job": true
            }
          }
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 14
      }
    },
    {
      "id": 9,
      "title": "Database Write Performance",
      "type": "graph",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(twitch_scraper_db_batch_insert_duration_seconds_bucket[5m]))",
          "legendFormat": "p95",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.50, rate(twitch_scraper_db_batch_insert_duration_seconds_bucket[5m]))",
          "legendFormat": "p50",
          "refId": "B"
        }
      ],
      "yaxes": [
        {
          "format": "s",
          "label": "Duration"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 22
      }
    },
    {
      "id": 10,
      "title": "Database Batch Size",
      "type": "graph",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(twitch_scraper_db_batch_size_bucket[5m]))",
          "legendFormat": "p95",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.50, rate(twitch_scraper_db_batch_size_bucket[5m]))",
          "legendFormat": "p50",
          "refId": "B"
        }
      ],
      "yaxes": [
        {
          "format": "short",
          "label": "Batch Size"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 22
      }
    },
    {
      "id": 11,
      "title": "Database Messages Written",
      "type": "stat",
      "targets": [
        {
          "expr": "twitch_scraper_db_messages_written_total",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          },
          "unit": "short"
        }
      },
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 0,
        "y": 30
      }
    },
    {
      "id": 12,
      "title": "Database Write Errors",
      "type": "stat",
      "targets": [
        {
          "expr": "twitch_scraper_db_write_errors_total",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "red",
                "value": 1
              }
            ]
          },
          "unit": "short"
        }
      },
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 6,
        "y": 30
      }
    },
    {
      "id": 13,
      "title": "Message Buffer Utilization",
      "type": "gauge",
      "targets": [
        {
          "expr": "twitch_scraper_message_buffer_utilization_percent",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 50
              },
              {
                "color": "red",
                "value": 80
              }
            ]
          },
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      },
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 12,
        "y": 30
      }
    },
    {
      "id": 14,
      "title": "Messages Dropped",
      "type": "stat",
      "targets": [
        {
          "expr": "twitch_scraper_messages_dropped_total",
          "refId": "A"
