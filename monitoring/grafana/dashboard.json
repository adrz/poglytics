      "type": "stat",
      "targets": [
        {
          "expr": "sum(twitch_scraper_connection_reconnects_total)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
