{
  "id": null,
  "title": "Twitch Chat Scraper Dashboard",
  "tags": ["twitch", "scraper", "prometheus"],
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "title": "Total Messages Received",
      "type": "stat",
      "targets": [
        {
          "expr": "twitch_scraper_messages_total",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "title": "Messages Per Second",
      "type": "graph",
      "targets": [
        {
          "expr": "rate(twitch_scraper_messages_total[5m])",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      }
    },
    {
